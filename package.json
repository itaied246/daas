{
  "name": "django-gen",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "clean": "rimraf build",
    "start": "yarn run clean && yarn build && node build/index.js",
    "build": "tsc -p tsconfig.json",
    "build:dev": "yarn build -- -w",
    "test": "jest --coverage && codecov",
    "test:dev": "jest --watchAll"
  },
  "devDependencies": {
    "@types/jest": "^20.0.2",
    "@types/node": "^8.0.10",
    "codecov": "^2.2.0",
    "jest": "^20.0.4",
    "rimraf": "^2.6.1",
    "ts-jest": "^20.0.7",
    "typescript": "^2.4.1"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(tsx?|jsx?|ts?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json",
      "jsx"
    ]
  }
}
